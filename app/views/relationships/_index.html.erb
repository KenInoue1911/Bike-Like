<% users.each do |user| %>
<%= link_to user_path(user) do %>
<%= user.name %>
<% end %>
フォロー数: <%= user.followings.count %>
フォロワー数: <%= user.followers.count %>
    <% if current_user != user %>
    <div class="follow_<%= user.id %>">
      <%= render 'relationships/follow-btn', user: user %>
      <% end %>
    </div>
    

<% end %>